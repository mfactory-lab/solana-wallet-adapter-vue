{
  "name": "@solana/wallet-adapter-vue",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.14.2",
  "description": "Solana wallet integration for Vue 3",
  "author": "Loris Leiva",
  "license": "Apache-2.0",
  "repository": "https://github.com/mfactory-labs/solana-wallet-adapter-vue",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/wallet-adapter-vue.js",
      "require": "./dist/wallet-adapter-vue.cjs"
    },
    "./styles.css": "./styles.css"
  },
  "main": "./dist/wallet-adapter-vue.js",
  "module": "./dist/wallet-adapter-vue.cjs",
  "files": [
    "dist",
    "src",
    "styles.css"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "demo": "vite dev -c vite.demo.config.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.95.5",
    "vue": "*"
  },
  "dependencies": {
    "@solana-mobile/wallet-adapter-mobile": "^2.1.4",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-standard-wallet-adapter-base": "^1.1.2",
    "@vueuse/core": "^11.3.0",
    "@wallet-standard/app": "^1.1.0",
    "@wallet-standard/base": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.10.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint-plugin-format": "^0.1.2",
    "typescript": "5.6.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-native",
        "react-dom"
      ]
    },
    "overrides": {
      "elliptic@<6.6.0": ">=6.6.0"
    }
  }
}
